<body>

<!-- navigation and insignia -->
<div class="nav-bar">
	
	<div class="signia">
		<a id="logo" href="/"><i class='fas fa-user-md' style='font-size:32px'></i>  DOCHERE
		</a>
	</div>

	<div class="signia">
		<ul>
		  <li><img src="/user.png" title="<%=user%>" width="25" id="user"></li>
		  <li><a class="active" href="#">HOME</a></li>
      <li><a href="/manageappointment/?user=<%=user%>">MANAGE APPOINTMENT</a></li>
		</ul>
	</div>

</div>
<!-- navigation and insignia -->

<!--  box find doctor near you  -->
<div class="form">
<form action="/?user=<%=user%>" class="w3-onethird w3-row-padding w3-rgb(192,192,192) w3-text-black w3-margin" method="post">
<h2 class="w3-center" id="doctorid">Find a doctor near you</h2>
 
<div class="w3-row w3-section">
  <div class="w3-half" style="width:50px"><i class="w3-xxlarge fa fa-user"></i></div>
    <div class="w3-rest">
      <input class="w3-input w3-border" name="doctorname" type="text" placeholder="doctorname" id="doctype">
    </div>
</div>

<div class="w3-row w3-section">
  <div class="w3-half" style="width:50px"><i class="w3-xxlarge fa fa-location-arrow"></i>
  </div>
    <div class="w3-rest">
      <input class="w3-input w3-border" name="zipcode" type="text" pattern="[0-9]{6}" placeholder="zip code" maxlength="6">
    </div>
</div>
